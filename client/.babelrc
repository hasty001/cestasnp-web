{
  "presets": [
    "@babel/react",
    [
      "@babel/preset-env",
      {
       "useBuiltIns": "entry",
       "corejs": "3.5"
      }
     ]
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": true
      }
    ],
    ["@babel/plugin-transform-for-of"]
  ]
}